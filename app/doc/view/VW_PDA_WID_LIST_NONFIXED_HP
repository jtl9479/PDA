[홈플러스 비정량 제품 계근 사용]

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "INNO"."VW_PDA_WID_LIST_NONFIXED_HP" ("GI_H_ID", "GI_D_ID", "EOI_ID", "ITEM_CODE", "ITEM_NAME", "EMARTITEM_CODE", "EMARTITEM", "GI_REQ_PKG", "GI_REQ_QTY", "AMOUNT", "GOODS_R_ID", "GR_REF_NO", "GI_REQ_DATE", "BL_NO", "BRAND_CODE", "BRANDNAME", "CLIENT_CODE", "CLIENTNAME", "CENTERNAME", "ITEM_SPEC", "CT_CODE", "PACKER_CODE", "IMPORT_ID_NO", "PACKERNAME", "PACKER_PRODUCT_CODE", "BARCODE_TYPE", "ITEM_TYPE", "PACKWEIGHT", "BARCODEGOODS", "STORE_IN_DATE", "GR_WAREHOUSE_CODE", "EMARTLOGIS_CODE", "EMARTLOGIS_NAME", "WH_AREA", "USE_NAME", "USE_CODE", "CT_NAME", "STORE_NAME", "STORE_CODE") AS
  SELECT  -- ID.STATUS ,
ID.GI_H_ID
, ID.GI_D_ID
, ID.EOI_ID
, ID.ITEM_CODE
--, BEB.EMRTITEM_NAME  ITEM_NAME
, EOI.ITEM_NAME  ITEM_NAME     -- 발주서의 상품명으로 변경함.
, EOI.ITEM_CODE EMARTITEM_CODE
-- , BEB.EMRTITEM_NAME  EMARTITEM
, EOI.ITEM_NAME  EMARTITEM   -- 발주서의 상품명으로 변경함.
, ID.GI_REQ_PKG
, ID.GI_REQ_QTY
, ID.AMOUNT
, ID.GOODS_R_ID
, R.GR_REF_NO
, IH.GI_REQ_DATE
, ID.BL_NO
, ID.BRAND_CODE
, INNO.DE_COMMON('BRAND', ID.BRAND_CODE) BRANDNAME
, IH.CLIENT_CODE
, EOI.STORE_NAME  CLIENTNAME
, DE_COMMON('HOMEPLUS_STORE_CODE',EOI.CENTER_CODE)   CENTERNAME
, R.ITEM_SPEC
, R.CT_CODE
, 'IN67677' PACKER_CODE  -- IN67677 -- IN61566
, R.IMPORT_ID_NO
, '하이랜드이노베이션' PACKERNAME
, ID.ITEM_CODE PACKER_PRODUCT_CODE
, BEB.BARCODE_TYPE
, BEB.ITEM_TYPE
, NULL  AS  PACKWEIGHT
, ID.ITEM_CODE   BARCODEGOODS
, EOI.STORE_IN_DATE
, R.GR_WAREHOUSE_CODE
,' ' EMARTLOGIS_CODE
, ' ' EMARTLOGIS_NAME
, ' ' WH_AREA
, ' ' USE_NAME
, ' ' USE_CODE
, (SELECT BC.CT_NAME  FROM B_COUNTRY bc WHERE BC.CT_CODE = R.CT_CODE) AS CT_NAME
, EOI.STORE_NAME
, EOI.STORE_CODE
FROM W_GOODS_IH IH INNER JOIN W_GOODS_ID ID ON IH.GI_H_ID = ID.GI_H_ID
                   INNER JOIN W_E_ORDER_ITEM EOI ON ID.EOI_ID = EOI.EOI_ID
                   INNER JOIN B_EMART_BARCODE beb ON EOI.ITEM_CODE = BEB.EMARTITEM_CODE
                   INNER JOIN W_GOODS_R R ON R.GOODS_R_ID = ID.GOODS_R_ID
                   INNER JOIN B_ITEM BI ON BI.ITEM_CODE = ID.ITEM_CODE
WHERE 1=1
AND ID.STATUS = '10'
AND  BEB.BARCODE_TYPE = 'H5'
AND  R.GR_DATE  > '20240101'
ORDER BY BEB.EMRTITEM_NAME , EOI.STORE_CODE   -- 정렬 추가함
;
